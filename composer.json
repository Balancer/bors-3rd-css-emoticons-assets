{
	"name": "balancer/bors-3rd-css-emoticons-assets",
	"description": "CSS Emoticons assets",
	"license": "Dual licensed under the MIT and GPL licenses",

	"authors": [
		{
			"name": "Balancer",
			"email": "balancer@balancer.ru"
		}
	],

	"require": {
		"atelierspierrot/assets-manager": "1.*"
	},

	"type": "bors-assets",

    "autoload": {
		"classmap": ["classes/"]
    },

	"extra": {
		"assets-presets": {
			"jquery.cssemoticons": {
				"css": "/css/jquery.cssemoticons.css",
				"jsfiles_footer": "/js/jquery.cssemoticons.min.js",
				"jquery_plugin": "cssemoticons"
			},
			"jquery.css-emoticons": {
				"css": "/css/jquery.cssemoticons.css",
				"jsfiles_footer": "/js/jquery.cssemoticons.min.js",
				"jquery_plugin": "cssemoticons"
			},
			"jquery.css-emoticons-nf": {
				"css": "/css/jquery.cssemoticons-nf.css",
				"jsfiles_footer": "/js/jquery.cssemoticons.nf.min.js",
				"jquery_plugin": "cssemoticons.nf"
			}
		}
	}
}
